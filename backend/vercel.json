{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node",
        "runtime": "nodejs18.x"
        }],
        "functions": {
            "api/**/*.js": {
                "runtime": "nodejs18.x",
                "memory": 1024,
                "maxDuration": 10
            }
        },
    "routes": [{
        "src": "/(.*)",
        "dest": "/"
    }]
}